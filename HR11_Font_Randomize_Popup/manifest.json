{
    "manifest_version": 2,
    "name": "Demo Extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "popup.js",
                "getDocUrl.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": {
            "16": "images/pen16.png",
            "48": "images/pen48.png",
            "128": "images/pen128.png"
        },
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/pen16.png",
        "48": "images/pen48.png",
        "128": "images/pen128.png"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "externally_connectable": {
        "matches": [
            "*://*.youtube.com/*",
            "*://docs.google.com/*"
        ]
    }
}